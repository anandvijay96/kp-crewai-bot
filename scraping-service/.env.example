# Bun Scraping Service Configuration

# Server Configuration
PORT=3001
NODE_ENV=production
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

# Python Backend Integration
PYTHON_BACKEND_URL=http://localhost:8001

# Search Engine APIs (optional)
GOOGLE_API_KEY=your_google_custom_search_api_key_here
GOOGLE_CSE_ID=your_google_custom_search_engine_id_here
BING_API_KEY=your_bing_web_search_api_key_here

# Logging and Debug (optional)
LOG_LEVEL=info
DEBUG_MODE=false

# Browser Configuration (optional overrides)
BROWSER_HEADLESS=true
BROWSER_STEALTH=true
BROWSER_TIMEOUT=60000
